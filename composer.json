{
    "name": "lugark/calendar-generator",
    "description": "to generate pdf calendars with custom events & holidays",
    "type": "project",
    "license": "MIT",
    "minimum-stability": "dev",
    "require": {
        "php": "^7.4",
        "ext-curl": "*",
        "ext-json": "*",
        "aeon-php/calendar": "^1.0.0",
        "doctrine/annotations": "^1.0",
        "lugark/calendar-pdf-renderer": "dev-master",
        "phpdocumentor/reflection-docblock": "^5.2",
        "rybakit/msgpack": "^0.9.0",
        "setasign/fpdf": "^1.8",
        "symfony/console": "5.4.*",
        "symfony/dom-crawler": "5.4.*",
        "symfony/dotenv": "5.4.*",
        "symfony/flex": "^1.12",
        "symfony/framework-bundle": "5.4.*",
        "symfony/property-access": "5.4.*",
        "symfony/property-info": "5.4.*",
        "symfony/serializer": "5.4.*",
        "symfony/yaml": "5.4.*"
    },
    "require-dev": {
        "phpstan/phpstan": "1.8.x-dev",
        "symfony/phpunit-bridge": "^5.2"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.3.*"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/lugark/calendar-pdf-renderer.git"
        }
    ]
}
